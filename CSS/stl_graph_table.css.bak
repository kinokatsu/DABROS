}
/*jQuery ToolTip用*/
#tooltip {
	position: absolute;
	border:3px ridge #DD3;	/*#666;*/
	background: #FFFFCC;	/*#DC3;*/
	color: #11C;
	padding: 8px;
	opacity: 0.9;
	font-size:8pt;
	font-weight:normal;
	/* ▼角丸 */
	-webkit-border-radius: 5px; /* Chrome,Safari */
	-moz-border-radius: 5px;    /* Firefox */
	border-radius: 5px;         /* CSS3 */
	-moz-box-shadow: 3px -3px 6px #444 ,3px 3px 6px #444,-3px 3px 6px #444,-3px -3px 6px #444;     /* Firefox用*/
	-webkit-box-shadow: 2px -2px 6px #444 ,3px 3px 6px #444,-2px 2px 6px #444,-2px -2px 6px #444;  /*Safari,Google Chrome用*/
	box-shadow: #444 2px -2px 6px, #444 3px 3px 6px, #444 -2px 2px 6px, #444 -2px -2px 6px; /* 10 10 30 */
}
